   "source": [
    "max_char = 50\n",
    "line = line1.strip()\n",
    "no_char = int(len(line))            # returns a number of charcters in line \n",
    "no_blanks = MAXCHAR - no_char\n",
    "\n",
    "result = \" \" * no_blanks\n",
    "result += line\n",
    "\n",
    "print(\"1234567890\"*5)\n",
    "print(result)"
